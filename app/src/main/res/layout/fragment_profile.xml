            app:layout_constraintTop_toBottomOf="@+id/view" />

        <TextView
            android:id="@+id/textView11"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:text="INES Bank"
            android:textColor="@color/albastru"
            android:textSize="15dp"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/t1"
            app:layout_constraintEnd_toEndOf="@+id/view"
            app:layout_constraintTop_toTopOf="@+id/t1" />

        <TextView
            android:id="@+id/textView12"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:background="@drawable/logo"
            android:text=""
            app:layout_constraintBottom_toBottomOf="@+id/textView11"
            app:layout_constraintEnd_toStartOf="@+id/textView11"
            app:layout_constraintTop_toTopOf="@+id/textView11" />

        <TextView
            android:id="@+id/ochi"
            android:layout_width="20dp"
            android:layout_height="15dp"
            android:layout_marginEnd="20dp"
            android:background="@drawable/eye"
            android:text=""
            app:layout_constraintBottom_toBottomOf="@+id/t3"
            app:layout_constraintEnd_toEndOf="@+id/view"
            app:layout_constraintTop_toTopOf="@+id/t3"
            app:layout_constraintVertical_bias="0.406" />

        <TextView
            android:id="@+id/copy"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="20dp"
            android:text="COPY"
            app:layout_constraintBottom_toBottomOf="@+id/textView8"
            app:layout_constraintEnd_toEndOf="@+id/view"
            app:layout_constraintTop_toTopOf="@+id/textView8" />

        <TextView
            android:id="@+id/textView15"
            android:layout_width="20dp"
            android:layout_height="20dp"
            android:layout_marginEnd="5dp"
            android:background="@drawable/copy"
            android:text=""
            app:layout_constraintBottom_toBottomOf="@+id/copy"
            app:layout_constraintEnd_toStartOf="@+id/copy"
            app:layout_constraintTop_toTopOf="@+id/copy"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView16"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginBottom="8dp"
            android:text="Mastercard"
            android:textColor="@color/albastru"
            android:textStyle="italic"
            app:layout_constraintBottom_toBottomOf="@+id/view"
            app:layout_constraintEnd_toEndOf="@+id/textView5"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="@+id/textView5" />



    </androidx.constraintlayout.widget.ConstraintLayout>


</FrameLayout>